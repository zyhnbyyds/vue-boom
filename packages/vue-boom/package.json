{
  "name": "vue-boom",
  "type": "module",
  "version": "0.1.31",
  "private": false,
  "description": "Some ui components",
  "author": {
    "name": "zyujie",
    "url": "https://zyujie.me"
  },
  "repository": {
    "url": "https://github.com/zyhnbyyds/vue-boom.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --env.NODE_ENV=production",
    "dev": "vite --port 4000 --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "release": "bumpp && pnpm publish --no-git-checks",
    "prerelease": "pnpm lint && pnpm build",
    "uno": "unocss src/components/*.vue -m -o assets/styles/unocss.css"
  },
  "dependencies": {
    "@vueuse/core": "^13.3.0",
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/streamline-emojis": "^1.2.2",
    "@types/node": "^22.15.29",
    "@unocss/cli": "^66.1.4",
    "@unocss/eslint-config": "^66.1.3",
    "@unocss/reset": "^66.1.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^26.1.0",
    "tsdown": "^0.12.7",
    "typescript": "^5.8.3",
    "unocss": "^66.1.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue": "^6.2.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.1",
    "vue-tsc": "^2.2.10"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
