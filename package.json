{
  "name": "vue-boom",
  "type": "module",
  "private": true,
  "description": "Some ui components",
  "author": {
    "name": "zyujie",
    "url": "https://zyujie.me"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "url": "https://github.com/zyhnbyyds/vue-boom.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite playground --port 4000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "pnpm -r run release",
    "docs:dev": "vitepress dev docs --port 4001",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@vueuse/core": "^13.3.0",
    "vue": "^3.5.16",
    "vue-boom": "workspace:*",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/streamline-emojis": "^1.2.2",
    "@types/node": "^22.15.29",
    "@unocss/cli": "^66.1.4",
    "@unocss/eslint-config": "^66.1.4",
    "@unocss/reset": "^66.1.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.28.0",
    "jsdom": "^26.1.0",
    "pnpm": "^10.12.1",
    "typescript": "^5.8.3",
    "unocss": "^66.1.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue": "^6.2.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.1",
    "vue-tsc": "^2.2.10"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
