{
  "name": "vue-boom",
  "type": "module",
  "version": "0.2.6",
  "private": false,
  "description": "Some ui components",
  "author": {
    "name": "zyujie",
    "url": "https://zyujie.me"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "url": "https://github.com/zyhnbyyds/vue-boom.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm uno && tsdown",
    "dev": "vite --port 4000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs --port 4001",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "bumpp && pnpm publish --no-git-checks",
    "prerelease": "pnpm lint:fix && pnpm build",
    "uno": "unocss src/components/**/*.vue -o assets/styles/unocss.css"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.4.2",
    "@iconify-json/carbon": "^1.2.15",
    "@iconify-json/streamline-emojis": "^1.2.4",
    "@types/node": "^24.10.1",
    "@unocss/cli": "^66.5.10",
    "@unocss/eslint-config": "^66.5.10",
    "@unocss/reset": "^66.5.10",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/test-utils": "^2.4.6",
    "dayjs": "^1.11.19",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.1.0",
    "estree-walker": "^3.0.3",
    "jsdom": "^27.2.0",
    "pnpm": "^10.24.0",
    "tsdown": "^0.17.0",
    "typescript": "^5.9.3",
    "unocss": "^66.5.10",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue": "^7.1.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.6",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.6"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
